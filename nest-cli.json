{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
        "assets": [
          {
            "include": "i18n/**/*.json",
            "outDir": "dist/apps/api-gateway/src",
            "watchAssets": true
          }
        ]
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "firebase": {
      "type": "library",
      "root": "libs/firebase",
      "entryFile": "index",
      "sourceRoot": "libs/firebase/src",
      "compilerOptions": {
        "tsConfigPath": "libs/firebase/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "mail-service": {
      "type": "application",
      "root": "apps/mail-service",
      "entryFile": "main",
      "sourceRoot": "apps/mail-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mail-service/tsconfig.app.json",
        "assets": [
          {
            "include": "templates/**/*.hbs",
            "outDir": "dist/apps/mail-service/src"
          }
        ],
        "watchAssets": true
      }
    },
    "cloudinary": {
      "type": "library",
      "root": "libs/cloudinary",
      "entryFile": "index",
      "sourceRoot": "libs/cloudinary/src",
      "compilerOptions": {
        "tsConfigPath": "libs/cloudinary/tsconfig.lib.json"
      }
    },
    "payment-service": {
      "type": "application",
      "root": "apps/payment-service",
      "entryFile": "main",
      "sourceRoot": "apps/payment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-service/tsconfig.app.json"
      }
    },
    "payos": {
      "type": "library",
      "root": "libs/payos",
      "entryFile": "index",
      "sourceRoot": "libs/payos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/payos/tsconfig.lib.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "chat-service": {
      "type": "application",
      "root": "apps/chat-service",
      "entryFile": "main",
      "sourceRoot": "apps/chat-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-service/tsconfig.app.json"
      }
    },
    "ffmpeg": {
      "type": "library",
      "root": "libs/ffmpeg",
      "entryFile": "index",
      "sourceRoot": "libs/ffmpeg/src",
      "compilerOptions": {
        "tsConfigPath": "libs/ffmpeg/tsconfig.lib.json"
      }
    },
    "video-service": {
      "type": "application",
      "root": "apps/video-service",
      "entryFile": "main",
      "sourceRoot": "apps/video-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/video-service/tsconfig.app.json"
      }
    }
  }
}